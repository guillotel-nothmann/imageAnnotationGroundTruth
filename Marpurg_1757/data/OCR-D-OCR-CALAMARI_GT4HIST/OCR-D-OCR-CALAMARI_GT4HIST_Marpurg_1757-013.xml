            <pc:TextLine id="region_id_1_line0002">
                <pc:Coords points="472,586 2626,586 2626,684 472,684"/>
                <pc:TextEquiv conf="0.9987728595733643">
                    <pc:Unicode>Gebrauch ſelten iſt, in die Noten des zweyten Buches herunter</pc:Unicode>
