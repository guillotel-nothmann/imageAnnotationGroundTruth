            <pc:TextLine id="region_id_1_line0004">
                <pc:Coords points="468,809 2629,809 2629,913 468,913"/>
                <pc:TextEquiv conf="0.9924120306968689">
                    <pc:Unicode>kann, wenn man die im Texte vorgetragene weſentliche Regeln</pc:Unicode>
